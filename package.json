{
  "name": "react-rocket-boilerplate",
  "version": "0.1.0",
  "author": "Jake Marsh <jakemmarsh@gmail.com>",
  "description": "A boilerplate using ReactJS (along with React Router and RefluxJS), SASS, Gulp, and Browserify",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakemmarsh/react-rocket-boilerplate.git"
  },
  "keywords": [
    "gulp",
    "browserify",
    "reactjs",
    "refluxjs",
    "react-router",
    "sass",
    "boilerplate"
  ],
  "private": false,
  "engines": {
    "node": ">=0.12.x"
  },
  "devDependencies": {
    "browserify": "^5.10.0",
    "gulp": "^3.8.8",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.3.x",
    "run-sequence": "^0.3.6",
    "gulp-uglify": "^1.0.1",
    "vinyl-source-stream": "^0.1.1",
    "gulp-util": "^3.0.1",
    "watchify": "^1.0.2",
    "reactify": "^0.14.0",
    "gulp-if": "^1.2.4",
    "gulp-imagemin": "^2.0.0",
    "gulp-notify": "^1.6.0",
    "gulp-streamify": "0.0.5",
    "react": "^0.12.x",
    "react-router": "^0.11.x",
    "del": "^0.1.3",
    "morgan": "^1.5.1",
    "express": "^4.12.2",
    "browser-sync": "^2.2.2",
    "gulp-autoprefixer": "^2.1.0",
    "humps": "^0.4.2",
    "when": "^3.7.2",
    "reflux": "^0.2.5",
    "superagent": "^0.21.0",
    "lodash": "^3.3.1",
    "react-tools": "^0.12.2",
    "jest-cli": "^0.4.0"
  },
  "jest": {
    "rootDir": ".",
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testPathDirs": ["<rootDir>"],
    "testDirectoryName": "__tests__",
    "testFileExtensions": ["js"],
    "moduleFileExtensions": ["js", "json"],
    "unmockedModulePathPatterns": ["<rootDir>/node_modules/react"]
  },
  "scripts": {
    "test": "jest"
  }
}
